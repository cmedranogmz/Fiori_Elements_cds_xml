{
  "name": "Fiori_Elements",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/http-client": "^4.1.1",
    "@sap-cloud-sdk/util": "^4.1.1",
    "@sap/cds": "^9",
    "@sap/low-code-event-handler": "^1.2.5",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4.9.0",
    "express": "^4.21.2",
    "hdb": "^2.25.1",
    "passport": "^0.7.0"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/dev-cap-tools": "^1.31.0"
  },
  "scripts": {
    "start": "cds-serve --profile production",
    "start-dev": "cds-serve --profile dev",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-products_cap_cds_annotations": "cds watch --open products_cap_cds_annotations/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-products_xml_annotations": "cds watch --open products_xml_annotations/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-products_fpm": "cds watch --open products_fpm/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "[dev]": {
        "db": {
          "kind": "sqlite",
          "credentials": {
            "database": "db.sqlite"
          }
        },
        "auth": "dummy"
      },
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      }
    }
  },
  "lcap": true,
  "sapux": [
    "app/products_cap_cds_annotations",
    "app/products_xml_annotations",
    "app/products_fpm"
  ]
}
